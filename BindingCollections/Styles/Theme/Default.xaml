<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ButtonBackground" Color="LightGray"/>
    <SolidColorBrush x:Key="TextColor" Color="Black"/>
    <SolidColorBrush x:Key="ButtonHover" Color="Gray"/>
    <SolidColorBrush x:Key="ButtonPressed" Color="DarkGray"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBorder" Color="DarkGray"/>
    <SolidColorBrush x:Key="StackBackground" Color="LightBlue"/>
    <SolidColorBrush x:Key="GridBackground" Color="LightBlue"/>
</ResourceDictionary>