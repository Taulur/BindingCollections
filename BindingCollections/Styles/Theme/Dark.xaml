<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ButtonBackground" Color="DarkGray"/>
    <SolidColorBrush x:Key="TextColor" Color="White"/>
    <SolidColorBrush x:Key="ButtonHover" Color="DarkSlateGray"/>
    <SolidColorBrush x:Key="ButtonPressed" Color="LightSlateGray"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBorder" Color="Black"/>
    <SolidColorBrush x:Key="StackBackground" Color="DarkSlateGray"/>
    <SolidColorBrush x:Key="GridBackground" Color="DarkSlateGray"/>
</ResourceDictionary>